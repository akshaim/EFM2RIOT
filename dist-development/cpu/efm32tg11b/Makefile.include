export CPU_ARCH = cortex-m0plus
export CPU_FAM = efm32tg11b
export CPU_SERIES = 1

# CMSIS-DSP is needed for arm_math.h
USEPKG += cmsis-dsp

# include vendor device headers
export INCLUDES += -I$(RIOTCPU)/efm32tg11b/include/vendor

# include cortexm_common
include $(RIOTMAKE)/arch/cortexm.inc.mk

# include efm32_common
include $(RIOTCPU)/efm32_common/Makefile.include
